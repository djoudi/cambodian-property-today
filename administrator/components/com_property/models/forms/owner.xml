                <field name="commune"
			type="text"
			label="Commune"
			description="Owner commune"
			size="40"
			default=""
		/>
                <field name="district"
			type="text"
			label="District"
			description="Owner district"
			size="40"
			default=""
		/>
                <field name="tel1"
			type="text"
			label="Telephone 1"
			description="Owner Telephone 1"
			size="40"
			default=""
		/>
                <field name="tel2"
			type="text"
			label="Telephone 2"
			description="Owner Telephone 2"
			size="40"
			default=""
		/>
                <field name="fax"
			type="text"
			label="Fax"
			description="Owner fax"
			size="40"
			default=""
		/>
                <field name="email"
			type="text"
			label="Email address"
			description="Owner email address"
			size="40"
			default=""
		/>
</form>
